@model IEnumerable<AppliTrAc.Models.Response>

    @{
    ViewBag.Title = "Seat Details";
    }

    <h2>Seats</h2>
<dl class="dl-horizontal">
        <dd>
            <table class="table">
                <tr>
                    <th>Module Id</th>
                    <th>Date</th>
                    <th>Seat Position</th>
                </tr>
                @foreach (var item in Model)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Survey.ModuleID)
                    </td> <td>
                        @Html.DisplayFor(modelItem => item.Date.ToShortTimeString())
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Value)
                    </td> <td>
                    </td>
                    <td>
                    </td>
                </tr>
                }
            </table>
        </dd>
    </dl>
    <p>
        @Html.ActionLink("Back to List", "Index")
    </p>

