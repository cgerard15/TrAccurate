@model IEnumerable<AppliTrAc.Models.Enrollment>

    @{
    ViewBag.Title = "Seat Details";
    }


<h2 class="well well-small">Enrollments</h2>
<dl class="dl-horizontal">
        <dd>
            <table class="table">
                <tr>
                    <th>Module Id</th>
                    <th>Module Name</th>
                </tr>
                @foreach (var item in Model)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.ModuleID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Module.ModuleName)
                    </td>
                    <td>
                        @Html.ActionLink("View Seats", "ModuleSeats", new {id = item.ModuleID, stId = item.StudentID})
                    </td>
                    <td></td>
                    <td></td>
                </tr>
                }
            </table>
        </dd>
    </dl>
    <p>
        @Html.ActionLink("Back to List", "Index")
    </p>

